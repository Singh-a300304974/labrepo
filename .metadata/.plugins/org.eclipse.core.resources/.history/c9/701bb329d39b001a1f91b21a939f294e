package myjavaproject;

public class myAppTester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub 
	   String x= "aman";
	   int age=20;
	   display(x,age);
	   displayname(x);
	   System.out.println("Hello my name is aman");
	   

	}
	public static void display (String n,int v)
	{
		System.out.println("my name is"+n+"my age is"+v);
		
	}

}
public static void displayname(String h)
{
	
	System.out.println("only display my name"+h);
	
}
}
